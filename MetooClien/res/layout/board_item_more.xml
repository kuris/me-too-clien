<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="wrap_content">
    <RelativeLayout android:id="@+id/board_item_more_mainLayout" android:layout_width="fill_parent" android:layout_height="55dp">
    	<Button android:id="@+id/board_item_more_backgroundButton" 
        	android:onClick="onClickMoreButton" android:layout_width="fill_parent" android:background="@null" android:layout_height="55dp"/>
        <LinearLayout android:id="@+id/board_item_more_linearLayout2" android:layout_centerHorizontal="true" android:layout_width="fill_parent" android:gravity="center" android:layout_height="55dp">
            <ProgressBar android:id="@+id/board_item_more_progressBar" android:visibility="invisible" android:layout_marginRight="5dp" android:layout_height="30dp" android:layout_width="30dp"/>
            <TextView android:layout_height="wrap_content" android:text="@string/board_item_more_updated" android:textSize="13.3sp" 
            	android:id="@+id/board_item_more_updateTextView" android:textColor="#9fb7c7" 
            	android:gravity="center" android:layout_width="wrap_content"/>
        </LinearLayout>
